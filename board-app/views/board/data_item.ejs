<% if (val != null) { %>
<tr class="row">
  <th class="col-2">
    <a href="/board/home/<%= val.User.name %>/<%= val.userId %>/0"
      ><%= val.User.name %></a
    >
  </th>
  <td class="col-7"><%= val.message %></td>
  <% let d = new Date(val.createdAt) let dstr = d.getFullYear() + '-' +
  (d.getMonth*( + 1) + '-' + d.getDate() + '-' + d.getHours() + ':' +
  d.getMinutes() + ':' + d.getSeconds() %>
  <td class="col-3"><%= dstr %></td>
</tr>
<% } %>
